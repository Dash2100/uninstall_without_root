{
  "name": "uninstall-without-root",
  "version": "1.0.0",
  "description": "Uninstall android app without root",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "electron",
    "desktop",
    "application"
  ],
  "author": "Dash2100",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.1.0",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.dash2100.uninstallwithoutroot",
    "productName": "Uninstall Without ROOT",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}